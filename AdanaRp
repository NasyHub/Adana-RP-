print("CategoryBar Frame oluşturuldu")

local isFlying = false
local flyVelocity
local tbl_upvr = { F = 0, B = 0, L = 0, R = 0, U = 0, D = 0 }
local function getC_upvr() local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait(); return Character end
local function hrp_upvr(arg1) return arg1:FindFirstChild("HumanoidRootPart") end
local function startFlying() local hrp = hrp_upvr(getC_upvr()); if hrp and not flyVelocity then flyVelocity = Instance.new("BodyVelocity", hrp); flyVelocity.MaxForce = Vector3.new(100000, 100000, 100000); flyVelocity.Velocity = Vector3.new(0, 0, 0); isFlying = true; print("Uçma aktif") end end
local function stopFlying() isFlying = false; if flyVelocity then flyVelocity:Destroy(); flyVelocity = nil end; print("Uçma devre dışı") end
local function updateFlying() if isFlying and flyVelocity then local hrp = hrp_upvr(getC_upvr()); if hrp then local v = Vector3.new(tbl_upvr.R - tbl_upvr.L, tbl_upvr.U - tbl_upvr.D, tbl_upvr.F - tbl_upvr.B); if v.Magnitude > 0 then local cam = workspace.CurrentCamera; flyVelocity.Velocity = (cam.CFrame.LookVector * v.Z + cam.CFrame.RightVector * v.X + Vector3.new(0, v.Y, 0)).Unit * 50 else flyVelocity.Velocity = Vector3.new(0, 0, 0) end end end end

UserInputService.InputBegan:Connect(function(arg1, arg2) if not arg2 then if arg1.KeyCode == Enum.KeyCode.W then tbl_upvr.F = 1 elseif arg1.KeyCode == Enum.KeyCode.S then tbl_upvr.B = 1 elseif arg1.KeyCode == Enum.KeyCode.A then tbl_upvr.L = 1 elseif arg1.KeyCode == Enum.KeyCode.D then tbl_upvr.R = 1 elseif arg1.KeyCode == Enum.KeyCode.Space then tbl_upvr.U = 1 elseif arg1.KeyCode == Enum.KeyCode.LeftControl then tbl_upvr.D = 1 end end end)
print("InputBegan bağlantısı oluşturuldu")
UserInputService.InputEnded:Connect(function(arg1) if arg1.KeyCode == Enum.KeyCode.W then tbl_upvr.F = 0 elseif arg1.KeyCode == Enum.KeyCode.S then tbl_upvr.B = 0 elseif arg1.KeyCode == Enum.KeyCode.A then tbl_upvr.L = 0 elseif arg1.KeyCode == Enum.KeyCode.D then tbl_upvr.R = 0 elseif arg1.KeyCode == Enum.KeyCode.Space then tbl_upvr.U = 0 elseif arg1.KeyCode == Enum.KeyCode.LeftControl then tbl_upvr.D = 0 end end)
print("InputEnded bağlantısı oluşturuldu")

local isNoClip = false
local noclipConnection
local function toggleNoClip() isNoClip = not isNoClip; if isNoClip then noclipConnection = RunService.Stepped:Connect(function() local char = LocalPlayer.Character; if char then for _, p in pairs(char:GetDescendants()) do if p:IsA("BasePart") then p.CanCollide = false end end end end); print("NoClip aktif") else if noclipConnection then noclipConnection:Disconnect(); noclipConnection = nil end; local char = LocalPlayer.Character; if char then for _, p in pairs(char:GetDescendants()) do if p:IsA("BasePart") then p.CanCollide = true end end end; print("NoClip devre dışı") end end
LocalPlayer.CharacterAdded:Connect(function(char) if isNoClip then for _, p in pairs(char:GetDescendants()) do if p:IsA("BasePart") then p.CanCollide = false end end end end)
print("NoClip fonksiyonları tanımlandı")

local speedValue = 16
local function updateSpeed() local char = LocalPlayer.Character; if char then local hum = char:FindFirstChildOfClass("Humanoid"); if hum then hum.WalkSpeed = speedValue; print("Hız güncellendi: " .. speedValue) end end end
print("Hız güncelleme fonksiyonu tanımlandı")

local tpLocations = {Vector3.new(-170.05867, 137.291046, 93.5553589), Vector3.new(-152.610397, 137.437836, 224.884857), Vector3.new(122.002594, 137.346588, 165.845291), Vector3.new(204.798019, 137.4039, 124.258606), Vector3.new(141.827423, 137.410828, 171.572113), Vector3.new(209.541199, 137.419098, 27.894062), Vector3.new(40.7069893, 137.499207, -137.850235), Vector3.new(32.485611, 137.573639, -274.915314), Vector3.new(10.1736879, 137.573639, -274.776398), Vector3.new(199.517014, 137.487442, -131.917343), Vector3.new(202.898529, 137.464478, -63.1280212), Vector3.new(288.16571, 137.47673, 350.711243), Vector3.new(309.066345, 137.476715, 345.732758), Vector3.new(393.492676, 137.636627, 564.548767), Vector3.new(18.3336449, 137.754517, 515.005737)}
local currentTpIndex = 0
local function teleportTo(index) local char = LocalPlayer.Character; if char and tpLocations[index] then local hrp = char:FindFirstChild("HumanoidRootPart"); if hrp then hrp.CFrame = CFrame.new(tpLocations[index]); print("Işınlanılan kasa: " .. index) end end end
print("Teleport fonksiyonları tanımlandı")

local gunShopCFrame = CFrame.new(113.733871, 137.530487, 195.422028, 0.9992764, 3.70870019e-08, 0.038035199, -3.75958251e-08, 1, 1.2662408e-08, 0)
local function teleportToGunShop() local char = LocalPlayer.Character; if char then local hrp = char:FindFirstChild("HumanoidRootPart"); if hrp then hrp.CFrame = gunShopCFrame; print("Silahçıya ışınlanıldı") end end end
print("GunShop teleport fonksiyonu tanımlandı")

local isAimbotOn = false
local function updateAimbot() if isAimbotOn then local char = LocalPlayer.Character; if char then local hrp = char:FindFirstChild("HumanoidRootPart"); if hrp then local target, maxDist = nil, math.huge; for _, p in pairs(Players:GetPlayers()) do if p ~= LocalPlayer and p.Character then local tr = p.Character:FindFirstChild("HumanoidRootPart"); if tr then local dist = (hrp.Position - tr.Position).Magnitude; if dist < maxDist then maxDist = dist; target = tr end end end end; if target then workspace.CurrentCamera.CFrame = CFrame.lookAt(workspace.CurrentCamera.CFrame.Position, target.Position) end end end end end
print("Aimbot fonksiyonu tanımlandı")

local espConnections = {}
local isEspOn = false
local function createESP(p) if p == LocalPlayer then return end; local char = p.Character or p.CharacterAdded:Wait(); local h = char:WaitForChild("Head"); local rp = char:WaitForChild("HumanoidRootPart"); local bg = Instance.new("BillboardGui"); bg.Adornee = h; bg.Size = UDim2.new(0, 100, 0, 50); bg.StudsOffset = Vector3.new(0, 2, 0); bg.AlwaysOnTop = true; local tl = Instance.new("TextLabel"); tl.Size = UDim2.new(1, 0, 1, 0); tl.BackgroundTransparency = 1; tl.Text = p.Name; tl.TextColor3 = Color3.fromRGB(255, 0, 0); tl.TextSize = 14; tl.Font = Enum.Font.SourceSansBold; tl.Parent = bg; bg.Parent = h; local hl = Instance.new("Highlight"); hl.FillColor = Color3.fromRGB(255, 0, 0); hl.OutlineColor = Color3.fromRGB(255, 255, 255); hl.Parent = char; espConnections[p] = {Billboard = bg, Highlight = hl}; print("ESP oluşturuldu: " .. p.Name) end
local function removeESP(p) if espConnections[p] then if espConnections[p].Billboard then espConnections[p].Billboard:Destroy() end; if espConnections[p].Highlight then espConnections[p].Highlight:Destroy() end; espConnections[p] = nil; print("ESP kaldırıldı: " .. p.Name) end end
for _, p in pairs(Players:GetPlayers()) do if p ~= LocalPlayer and isEspOn then task.spawn(function() createESP(p) end) end end
Players.PlayerAdded:Connect(function(p) if p ~= LocalPlayer and isEspOn then p.CharacterAdded:Connect(function() createESP(p) end) end end)
Players.PlayerRemoving:Connect(removeESP)
print("ESP fonksiyonları tanımlandı")

