print("Minimize butonu oluşturuldu")

local cb = Instance.new("TextButton", t)
cb.Name = "Close"
cb.Position = UDim2.new(1, -25, 0, 2.5)
cb.Size = UDim2.new(0, 20, 0, 20)
cb.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
