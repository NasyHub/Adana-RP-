local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local LocalPlayer = Players.LocalPlayer

print("OE Hub Scripti Başlatılıyor...")
local g = Instance.new("ScreenGui")
g.Parent = LocalPlayer:WaitForChild("PlayerGui")
g.Name = "OEHUB"
print("ScreenGui oluşturuldu, Parent: PlayerGui")

local m = Instance.new("Frame", g)
m.Name = "Main"
m.Size = UDim2.new(0, 450, 0, 300)
m.Position = UDim2.new(0.5, -225, 0.5, -150)
m.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
m.BorderSizePixel = 0
Instance.new("UICorner", m).CornerRadius = UDim.new(0, 10)
print("Main Frame oluşturuldu")

local t = Instance.new("Frame", m)
t.Name = "Topbar"
t.Size = UDim2.new(1, 0, 0, 25)
t.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
t.BorderSizePixel = 0
Instance.new("UICorner", t).CornerRadius = UDim.new(0, 5)
print("Topbar Frame oluşturuldu")

local l = Instance.new("TextLabel", t)
l.Name = "Title"
l.Size = UDim2.new(1, -60, 1, 0)
l.BackgroundTransparency = 1
l.Text = "OE HUB"
l.TextColor3 = Color3.new(1, 1, 1)
l.TextScaled = true
l.Font = Enum.Font.SourceSansBold
l.TextXAlignment = Enum.TextXAlignment.Left
Instance.new("UIPadding", l).PaddingLeft = UDim.new(0, 8)
print("Title TextLabel oluşturuldu")

local mb = Instance.new("TextButton", t)
mb.Name = "Minimize"
mb.Position = UDim2.new(1, -50, 0, 2.5)
