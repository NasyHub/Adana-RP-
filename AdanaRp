            nb.BackgroundColor3 = isNoClip and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(0, 100, 255)
            TweenService:Create(nb, TweenInfo.new(0.2), {BackgroundTransparency = 0}):Play()
        end)
        print("Main: NoClip özelliği oluşturuldu")
        local s = Instance.new("Frame", cont)
        s.Size = UDim2.new(1, 0, 0, 30)
        s.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        Instance.new("UICorner", s).CornerRadius = UDim.new(0, 5)
