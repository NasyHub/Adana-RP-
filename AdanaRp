        el.Text = "ESP: KAPALI"
        Instance.new("UIPadding", el).PaddingLeft = UDim.new(0, 8)
        local eb = Instance.new("TextButton", ef)
        eb.Position = UDim2.new(0.75, 0, 0.1, 0)
        eb.Size = UDim2.new(0.2, 0, 0.8, 0)
        eb.BackgroundColor3 = Color3.fromRGB(100, 0, 255)
        eb.Text = "Aç/Kapat"
        eb.TextColor3 = Color3.new(1, 1, 1)
        eb.TextScaled = true
        Instance.new("UICorner", eb).CornerRadius = UDim.new(0, 5)
        eb.MouseButton1Click:Connect(function() 
            isEspOn = not isEspOn
            if isEspOn then 
                for _, p in pairs(Players:GetPlayers()) do 
                    if p ~= LocalPlayer then task.spawn(function() createESP(p) end) end 
                end
                el.Text = "ESP: AÇIK"
                eb.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
            else 
                for _, p in pairs(Players:GetPlayers()) do removeESP(p) end
                el.Text = "ESP: KAPALI"
                eb.BackgroundColor3 = Color3.fromRGB(100, 0, 255)
            end
            TweenService:Create(eb, TweenInfo.new(0.2), {BackgroundTransparency = 0}):Play()
            print("ESP: " .. (isEspOn and "AÇIK" or "KAPALI"))
        end)
        print("ESP özelliği oluşturuldu")
    end,
