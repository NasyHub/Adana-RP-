    ["ESP"] = function(cont) 
        local ef = Instance.new("Frame", cont)
        ef.Size = UDim2.new(1, 0, 0, 30)
        ef.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        Instance.new("UICorner", ef).CornerRadius = UDim.new(0, 5)
        local el = Instance.new("TextLabel", ef)
        el.Size = UDim2.new(0.7, 0, 1, 0)
        el.BackgroundTransparency = 1
        el.TextColor3 = Color3.new(1, 1, 1)
        el.TextScaled = true
        el.TextWrapped = true
